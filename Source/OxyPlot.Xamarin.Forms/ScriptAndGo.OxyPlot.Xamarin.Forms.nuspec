        <reference file="OxyPlot.Xamarin.iOS.dll" />
        <reference file="OxyPlot.Xamarin.Forms.dll" />
        <reference file="OxyPlot.Xamarin.Forms.Platform.iOS.dll" />
      </group>
      <group targetFramework="MonoAndroid90">
        <reference file="OxyPlot.Xamarin.Android.dll" />
        <reference file="OxyPlot.Xamarin.Forms.dll" />
        <reference file="OxyPlot.Xamarin.Forms.Platform.Android.dll" />
      </group>
    </references>
  </metadata>


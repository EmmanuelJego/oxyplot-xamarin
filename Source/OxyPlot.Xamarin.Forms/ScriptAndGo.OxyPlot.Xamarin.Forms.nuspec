<?xml version="1.0"?>
<package>
  <metadata>
    <id>ScriptAndGo.Oxyplot.Xamarin.Forms</id>
    <version>1.0.7</version>
    <authors>Script and Go</authors>
    <owners>Script and Go</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Bibliothèque pour créer des graphiques en tous genres. Gestion des gestures (pan, pinch, single tap, double tap).</description>
    <releaseNotes>First release.</releaseNotes>
    <dependencies>
      <dependency id="Xamarin.Forms" version="2.3.3.180" />
      <dependency id="OxyPlot.Core" version="1.0.0" />
    </dependencies>

